    <script>
        $(window).on('load', function () {
            if (feather) {
                feather.replace({
                    width: 14,
                    height: 14
                });
            }
            @*alert("@ViewData["UserRole"]");*@
        })


            function Preloader(action) {
                if (action == "show")
                {
                    $('body').preloader({ text: '', percent: 'Tazadam ', duration: '', zIndex: '99999', setRelative: false });
                }
                else if (action== "hide" )
                {
                    $('body').preloader('remove');
                }
            }


